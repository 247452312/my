<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="indi.uhyils.dao.DynamicCodeHistoryDao">
  <resultMap type="indi.uhyils.pojo.DO.DynamicCodeHistoryDO" id="DynamicCodeHistoryMap">
  </resultMap>

  <!--批量通过主键修改数据-->
  <update id="updateBatch" parameterType="indi.uhyils.pojo.DO.DynamicCodeHistoryDO">
    <foreach collection="list" item="item" index="index" open="begin" close=";end;" separator=";">
      update sys_dynamic_code_history set
      update_date = #{item.updateDate}
      ,update_user = #{item.updateUser}
              <if test="updateUser != null">
                ,update_user = #{updateUser}
              </if>
              <if test="serviceMark != null and serviceMark != ''">
                ,service_mark = #{serviceMark}
              </if>
              <if test="groupId != null">
                ,group_id = #{groupId}
              </if>
              <if test="remark != null and remark != ''">
                ,remark = #{remark}
              </if>
              <if test="createUser != null">
                ,create_user = #{createUser}
              </if>
              <if test="createDate != null">
                ,create_date = #{createDate}
              </if>
              <if test="deleteFlag != null">
                ,delete_flag = #{deleteFlag}
              </if>
              <if test="className != null and className != ''">
                ,class_name = #{className}
              </if>
              <if test="updateDate != null">
                ,update_date = #{updateDate}
              </if>
              <if test="content != null and content != ''">
                ,content = #{content}
              </if>
      where id = #{id}
    </foreach>
  </update>
</mapper>
